{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react", 
        "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
        "jsx-no-lambda": [
            false
        ],
        "jsx-no-multiline-js": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "interface-name": [
            false
        ],
        "jsdoc-format": [
            false
        ],
        "no-empty-interface": [
            false
        ]
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    }
}